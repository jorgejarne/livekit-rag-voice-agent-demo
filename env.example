# Copy this file to `.env.local` and fill in values.
# Never commit `.env.local`.

# --- LiveKit Cloud ---
LIVEKIT_URL=
LIVEKIT_API_KEY=
LIVEKIT_API_SECRET=

# --- LiveKit Agents model providers (used by src/agent.py) ---
# Google Gemini (LiveKit Agents inference wrapper)
GOOGLE_API_KEY=

# AssemblyAI STT (if you keep the default STT model)
ASSEMBLYAI_API_KEY=

# Cartesia TTS (if you keep the default TTS model)
CARTESIA_API_KEY=

# --- RAG / Qdrant ---
QDRANT_URL=
QDRANT_API_KEY=
QDRANT_COLLECTION_NAME=my_docs_vertex

# --- Embeddings (Vertex AI via service account fields) ---
GOOGLE_VERTEX_PROJECT_ID=
GOOGLE_VERTEX_LOCATION=
GOOGLE_VERTEX_MODEL_ID=text-embedding-004
GOOGLE_VERTEX_TOKEN_ENDPOINT=https://oauth2.googleapis.com/token
GOOGLE_VERTEX_EMAIL=
GOOGLE_VERTEX_PK_ID=
# IMPORTANT: keep newlines escaped as \n
GOOGLE_VERTEX_PK=

# --- Booking / Contact email (Supabase Edge Function) ---
SUPABASE_EMAIL_ENDPOINT=
SUPABASE_KEY=

# Demo option: if set, always send booking email to this address (even if user provides another)
BOOKING_FORCE_EMAIL_TO=


